.NonPrintable
	background transparent
	pointer-events none
	position relative
	text-align center
	speak none

	&::after
		content: ' '

	&::before
		font-weight 400
		position absolute
		text-align center
		opacity .20
		width 100%
		left 0

	&--space::before
		content '\2219'

	&--eol::before
		content '\00AC'
